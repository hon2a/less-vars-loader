    "jest": "^24.9.0",
    "less": "^3.10.3",
    "memory-fs": "^0.5.0",
    "prettier": "^1.19.1",
    "webpack": "^4.41.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.4",
