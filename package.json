    "jest": "^23.6.0",
    "less": "^3.9.0",
    "memory-fs": "^0.4.1",
    "prettier": "^1.14.2",
    "webpack": "^4.29.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
